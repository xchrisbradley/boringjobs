{
  "name": "boringjobs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend:start": "next start",
    "frontend:lint": "next lint",
    "frontend:build": "next build",
    "frontend:run": "next dev",
    "protocol:test": "forge test",
    "protocol:build": "cannon build cannonfile.toml",
    "protocol:run": "cannon boringjobs --registry-priority local",
    "protocol:deploy": "source .env.local && cannon build --chain-id $CHAIN_ID --provider-url $RPC_ENDPOINT --private-key $PRIVATE_KEY",
    "protocol:verify": "source .env.local && cannon verify boringjobs --api-key $ETHERSCAN_API_KEY --chain-id $CHAIN_ID",
    "protocol:inspect": "cannon inspect boringjobs:latest@main --write-deployments ./src/abis"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.5.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/themes": "^2.0.3",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.28.4",
    "abitype": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "ethereumjs-util": "^7.1.5",
    "lowdb": "^7.0.1",
    "next": "14.1.3",
    "permissionless": "^0.1.10",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "sonner": "^1.4.3",
    "swr": "^2.2.5",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "viem": "2.x",
    "wagmi": "^2.5.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@usecannon/cli": "^2.11.21",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
