{
  "name": "onchain-jobboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend:start": "next start",
    "frontend:lint": "next lint",
    "frontend:build": "next build",
    "frontend:run": "next dev",
    "protocol:test": "forge test",
    "protocol:build": "cannon build cannonfile.toml && cannon build cannonfile.consumer.toml",
    "protocol:run": "cannon greeter --registry-priority local",
    "protocol:deploy": "source .env.local && cannon build --chain-id $CHAIN_ID --provider-url $RPC_ENDPOINT --private-key $PRIVATE_KEY",
    "protocol:verify": "source .env.local && cannon verify greeter-foundry --api-key $ETHERSCAN_API_KEY --chain-id $CHAIN_ID"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lowdb": "^7.0.1",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@usecannon/cli": "^2.11.21",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
